    "express": "^4.19.2",
    "form-data": "^4.0.1",
    "hat": "0.0.3",
    "jsonwebtoken": "^9.0.2",
    "mailgun-js": "^0.22.0",
    "mailgun.js": "^10.2.4",
    "mongodb": "^6.10.0",
    "morgan": "^1.10.0"

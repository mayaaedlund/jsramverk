{
  "name": "ssr-editor",
  "version": "1.0.0",
  "description": "Starter project for DV1677 JSRamverk",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov mocha --exit",
    "coverage": "nyc --reporter=text --reporter=lcov mocha --exit",
    "posttest": "npm run eslint",
    "type": "module",
    "start": "node app.js",
    "clean": "rm -rf node_modules package-lock.json",
    "eslint": "eslint .",
    "watch": "nodemon app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.8",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "email-validator": "^2.0.4",
    "express": "^4.19.2",
    "form-data": "^4.0.1",
    "hat": "0.0.3",
    "jsonwebtoken": "^9.0.2",
    "mailgun-js": "^0.22.0",
    "mailgun.js": "^10.2.4",
    "mongodb": "^6.10.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@babel/register": "^7.25.9",
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "eslint": "^5.15.3",
    "eslint-plugin-react": "^7.12.4",
    "javascript-style-guide": "^1.0.1",
    "mocha": "^10.8.2",
    "node-html-parser": "^1.1.12",
    "node-mocks-http": "^1.16.1",
    "nyc": "^14.1.1",
    "sinon": "^19.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "exclude": [
      "test/**"
    ],
    "include": [
      "app.mjs",
      "routes/*.mjs",
      "backend/**/*.mjs"
    ],
    "extension": [
      ".js",
      ".mjs"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.5"
  }
}
